cat Gencode_lncRNAsv7_summaryTable_05_02_2012.tab | cut -f 1,2,3| perl -e 'while($l=<>){$l =~ s/[-:]/\t/g;print $l;}'|awk '{print $3,"\t",$4,"\t",$5,"\t",$1,"\t",$2}' > derrien_trans_coords.bed
