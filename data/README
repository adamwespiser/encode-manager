cat lncRNAdbExportSeq.fa|grep ">"|grep -i human | sed 's/>//g'| sed 's/\([Hh]uman\)//g'|sed 's/[\)\(]//g'| perl -e 'while(<>){ $_=~m/NR_[0-9]*/;print $&."\n";}' > lncRNAdbNrCodesHuman.list
grep hg19 /Users/adam/sandbox/lncDB/* | perl -e 'while($line=<>){$line=~s/,//g;if($line=~m/hg19 chr[0-9]:[0-9]*-[0-9]*/g){$out=$&;$out=~s/-/:/g;print $out."\n";} }'| sed 's/hg19 //g' > lncRnaDbWebScrapForCoords.list



geneNamesLNCRNA.tab ~ list of 'functional' lncRNA pulled from genenames website: http://www.genenames.org/cgi-bin/hgnc_downloads?title=Genefam+data&submit=submit&hgnc_dbtag=on&preset=genefam&status=Approved&status=Entry+Withdrawn&status_opt=2&=on&format=text&limit=&.cgifields=&.cgifields=chr&.cgifields=status&.cgifields=hgnc_dbtag&where=gd_gene_fam_name%20RLIKE%20'(%5E%7C%20)fLNCRNA(%24%7C%2C)'%20&order_by=gd_app_sym_sort
cat geneNamesLNCRNA.tab| perl -e 'while(<>){if($_=~m/NR_[0-9]*/g){print $&."\n"}}' > geneNamesLNCRNA.list
cat geneNamesLNCRNA.tab| cut -f 1|perl -e 'while(<>){if($_=~m/HGNC:[0-9]*/g){print $&."\n"}}' | tee geneNamesLNCRNA_hgnc_id.list


note use quote in wget grab command


lncdlist.csv & lncdlist_parsed.tab - lncdlist.csv is manually curated
cat ../data/lncdList.csv | cut -d ',' -f 1,2,3|sed 's/,/x0x0x/g'|perl -e 'while(my $line = <>){$line=~s/x0x0x/\t/g;print $line;}'| tee lncdList_parsed.tab 

funcLncRnaDb_scrape.txt
wget http://www.valadkhanlab.org/web/cgi-bin/browse.cgi?genome=human -O funcLncRnaDb_scrape.txt
cat funcLncRnaDb_scrape.txt | grep "<td>"|grep -B 1 -i "human"|grep -vi "human"|grep "td"|sed 's/<td>//g'|sed 's/<\/td>//g' > funcLncRnaDb_scrape.list


