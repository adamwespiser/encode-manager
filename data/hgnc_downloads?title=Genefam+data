  <script type="text/javascript"> 
    $(document).ready(function(){ 
        $("ul.sf-menu").superfish(); 
        // default text for main search box
        var websearch_def='Search Website';
        // default text for gene search box
        var genesearch_def='Search Genes';

        $(".gene-search-form #gntext").val(genesearch_def).css('color', '#f0b460');
        $('.gene-search-form #gntext').focus(function() {
            if( $('.gene-search-form #gntext').val() == genesearch_def ){
                 $('.gene-search-form #gntext').val('').css('color', '#000');
            }
        });
        $('.gene-search-form #gntext').blur(function() {
            if( $('.gene-search-form #gntext').val() == '' ){
                 $('.gene-search-form #gntext').val(genesearch_def).css('color', '#f0b460');
            }
        });
        $('.gene-search-form #gnsubmit')[0].title = 'Search';

        // adjust for control panel
        var cpwidth = $('#controlpanel').eq(0).width();
        if (cpwidth) $('body').css('margin-left', cpwidth);

            }); 
  </script>
